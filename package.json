  "scripts": {
    "start": "node ./dist/main/server.js",
    "debug": "nodemon -L --watch ./dist --inspect=0.0.0.0:9222 --nolazy ./dist/main/server.js",
    "watch": "tsc -w",
    "build": "rimraf dist && tsc",
    "dev": "sucrase-node src/main/server.ts",
    "up": "npm run build && docker-compose up -d",
    "down": "docker-compose down",
    "prepare": "husky install",
    "test": "jest --passWithNoTests --silent --noStackTrace --runInBand",
    "test:ci": "npm t -- --coverage",
    "test:integration": "npm t -- --watch -c jest-integration-config.js",
    "test:staged": "npm t -- --findRelatedTests",
    "test:unit": "npm t -- --watch -c jest-unit-config.js",
    "test:verbose": "jest --passWithNoTests --runInBand â€”no-cache"
  },
  "dependencies": {
    "bcrypt": "^5.1.1",
    "express": "4.17.1",
    "jsonwebtoken": "^8.5.1",
    "mongodb": "^3.7.4",
    "nodemon": "^2.0.22",
    "validator": "^13.11.0"
  },
  "devDependencies": {
    "@shelf/jest-mongodb": "^1.3.4",
    "@types/bcrypt": "^5.0.0",
    "@types/express": "4.17.2",
    "@types/jest": "^24.0.25",
    "@types/jsonwebtoken": "^8.3.7",
    "@types/mongodb": "^3.6.20",
    "@types/node": "^13.1.6",
    "@types/supertest": "^2.0.8",
    "@types/validator": "^13.11.1",
    "@typescript-eslint/eslint-plugin": "^6.5.0",
    "eslint": "^8.48.0",
    "eslint-config-standard-with-typescript": "^39.0.0",
    "eslint-plugin-import": "^2.28.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^6.1.1",
    "eslint-plugin-standard": "^5.0.0",
    "git-commit-msg-linter": "^5.0.4",
    "husky": "^8.0.3",
    "jest": "^24.9.0",
    "lint-staged": "^13.2.2",
    "rimraf": "^5.0.1",
    "sucrase": "^3.32.0",
    "supertest": "^4.0.2",
    "ts-jest": "^24.3.0",
    "typescript": "^3.7.4"
  },
  "engines": {
    "node": "16.x"
  }
